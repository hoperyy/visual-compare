{
    "name": "Visual Compare",
    "version": "1.0.1",
    "manifest_version": 2,
    "description": "Visual Compare for FEer and UEDer",
    "icons": {
        "16": "static/img/compare-16.png",
        "32": "static/img/compare-32.png",
        "48": "static/img/compare-48.png",
        "128": "static/img/compare-128.png"
    },

    "browser_action": {
        "default_icon": "static/img/compare-128.png",
        "default_title": "Visual Compare",
        "default_popup": "template/popup.html"
    },

    "background": {
        "persistent": true,
        "scripts": [
            "static/lib/jquery-3.1.1.min.js"
        ]
    },

    "permissions": [
        "tabs",
        "contextMenus",
        "cookies",
        "notifications",
        "alarms",
        "webNavigation",
        "\u003Call_urls\u003E"
    ],

    "web_accessible_resources": [
        "static/img/compare-src.png",
        "static/img/compare-16.png",
        "static/img/compare-32.png",
        "static/img/compare-48.png",
        "static/img/compare-128.png",
        "static/css/mod/visual-compare.css"
    ],

    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*",
                "file://*/*"
            ],
            "js": [
                "static/lib/jquery-3.1.1.min.js",

                "static/js/core/util.js",

                "static/js/mod/visual-compare.js"
            ],
            "run_at": "document_end",
            "all_frames": false
        }
    ],
    "content_security_policy": "style-src 'self' 'unsafe-inline';script-src 'self' 'unsafe-eval'; object-src 'self' "

}
